    "@storybook/test": "^8.4.2",
    "@vitejs/plugin-react": "^4.3.3",
    "@vitest/ui": "^2.1.5",
    "eslint-plugin-storybook": "^0.11.0",
    "markdownlint-cli2": "^0.15.0",
    "prop-types": "^15.8.1",
    "storybook": "^8.4.2",
