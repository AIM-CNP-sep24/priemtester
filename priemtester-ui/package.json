  "scripts": {
    "dev": "vite",
    "build": "vite build && cp -r build/static/**/* ../src/main/resources/static/react/",
    "preview": "vite preview",
    "test": "react-scripts test",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
